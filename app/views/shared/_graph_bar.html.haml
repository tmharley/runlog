%svg{height: bar_height + 45,
     width: 50,
     title: tooltip,
     'data-toggle': 'tooltip'}
  %g
    %rect.graph-bar{height: bar_height + 20, width: 50, rx: 5, ry: 5, y: 25}
    %text.graph-bar-label.graph-bar-label-category{x: 25, y: bar_height + 40, width: 50}
      = label_category
    %text.graph-bar-label.graph-bar-label-amount{x: 25, y: 20, width: 50}
      = label_amount